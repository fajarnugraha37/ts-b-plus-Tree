{
  "name": "ts-btree",
  "module": "index.ts",
  "type": "module",
  "private": true,
  "scripts": {
    "test:unit": "bun test tests/unit",
    "test:integration": "bun test tests/integration",
    "test:memory": "BPTREE_MEMORY_COUNT=10000 bun test tests/integration/memoryUsage.test.ts",
    "test:ci": "bun x tsc --noEmit && bun test tests/unit && BPTREE_MEMORY_COUNT=10000 bun test tests/integration/memoryUsage.test.ts",
    "example:crud": "bun run examples/basicCrud.ts",
    "example:persistence": "bun run examples/persistence.ts",
    "example:scan": "bun run examples/rangeScan.ts",
    "example:restore": "bun run examples/restorePoint.ts",
    "example:advanced": "bun run examples/advancedOps.ts",
    "bench": "bun test benchmarks",
    "cli": "bun run tools/cli.ts"
  },
  "devDependencies": {
    "@types/bun": "latest",
    "@types/node": "^22.9.0",
    "typescript": "^5.6.3"
  },
  "peerDependencies": {
    "typescript": "^5"
  },
  "dependencies": {
    "commander": "^14.0.2"
  }
}
